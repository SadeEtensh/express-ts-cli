{
  "name": "express-ts-prod",
  "version": "1.0.0",
  "description": "A CLI tool to generate feature-based and service-based Express.js boilerplate with TypeScript",
  "main": "dist/cli/index.js",
  "bin": {
    "express-ts": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "dev": "ts-node src/cli/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "express",
    "typescript",
    "boilerplate",
    "cli",
    "generator",
    "nodejs"
  ],
  "author": "Sade Shure Garmamo",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^18.7.18",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
